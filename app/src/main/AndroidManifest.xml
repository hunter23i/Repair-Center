<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.repaircenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RepairCenter">

        <activity
            android:name=".Client_list"
            android:parentActivityName=".MainMenu" />
        <activity android:name=".Client_Details"
            android:parentActivityName=".Client_list" />
        <activity
            android:name=".Phone_list"
            android:parentActivityName=".MainMenu" />
        <activity
            android:name=".Phone_Details"
            android:parentActivityName=".Phone_list"
            />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Ajouter_portable" />
        <activity android:name=".MainMenu" />
    </application>

</manifest>